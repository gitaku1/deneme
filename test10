curl -s -X POST http://localhost:8050/v1/completions \
  -H "Content-Type: application/json" \
  -d '{
        "model": "llama-3.2-1b",
        "prompt": "Merhaba dünya!",
        "max_tokens": 10
      }' | jq -r '.choices[0].text'


# Artık sadece "llama3" yazmanız yeterli
curl http://localhost:8050/v1/chat/completions \
  -H "Content-Type: application/json" \
  -d '{"model": "llama3", "messages": [{"role": "user", "content": "Test"}]}'
